@ARTICLE{Schnute_General_CJoFaAS1994,
  author = {Jon T.~Schnute},
  title = {A General Framework for Developing Sequential Fisheries Models},
  journal = {Canadian Journal of Fisheries and Aquatic Sciences},
  year = {1994},
  volume = {51},
  pages = {1676--1688},
  file = {Schnute_General_CJoFaAS1994.pdf:Schnute_General_CJoFaAS1994.pdf:PDF},
  owner = {mwheldon},
  review = {Presents a very general framework
	
	for state-space population models, expressed in terms of
	
	matrices. There are four key components, all depending on time, $t$:
	
	\begin{description}
	
	\item[$\vec{X}_t$] a state vector, dimension $d_X$: the underlying,
	perhaps
	
	unobservable state of the population.
	
	\item[$\vec{Y}_t$] an observation vector, dimension $d_Y$: observable,
	
	depends directly on $\vec{X}_t$.
	
	\item[$\vec{Z}_t$] a control vector, dimension $d_Z$: observable,
	not
	
	directly dependent on $\vec{X}_t$. In fact, $\vec{X}_t$ may depend
	
	on $\vec{Z}_t$.
	
	\item[$\vec{\Theta}$] a parameter vector, dimension $d_\Theta$: unobservable.
	
	\end{description}
	
	The deterministic state-space model consists of two functional
	
	relationships which embody the temporal structure and observaion
	
	process:
	
	\begin{align*}
	
	\vec{X}_t &= \vec{F}(\vec{X}_{t-1},\vec{Z}_t,\vec{\Theta}) \\
	
	\vec{Y}_t &= \vec{G}(\vec{X}_t,\vec{Z}_t,\vec{\Theta}).
	
	\end{align*}
	
	In the stochastic model, deterministic functions are replaced by
	
	probabilities:
	
	\begin{align*}
	
	&\Pr(\vec{X}_0 \cond \vec{Z}_0, \vec{\Theta}) \\
	
	&\Pr(\vec{X}_t \cond \vec{X}_{t-1} \vec{Z}_{t}, \vec{\Theta}),\ t>0
	
	\\
	
	&\Pr(\vec{Y}_t \cond \vec{X}_t,\vec{Z}_t,\vec{\Theta}).
	
	\end{align*}
	
	The normal model is a special case; with $\vec{\delta}_t \sim
	
	\dNorm_{d_x}(\vec{0}, \vec{U}(\vec{Z}_t,\vec{\Theta}))$ and
	
	$\vec{\epsilon}_t \sim \dNorm_{d_T}(\vec{0}, \vec{V}(\vec{Z}_t,\vec{\Theta}))$:
	
	\begin{align*}
	
	\vec{X}_t &= \vec{F}(\vec{X}_{t-1},\vec{Z}_t,\vec{\Theta}_t) +
	
	\vec{\delta}_t \\
	
	\vec{Y}_t &= \vec{G}(\vec{X}_t,\vec{Z}_t,\vec{\Theta}_t) + \vec{\epsilon}_t.
	
	\end{align*}
	
	In the normal, linear model, $\vec{F}$ and $\vec{G}$ are further
	
	constrained to be linear functions.
	
	
	Several methods of inference are discussed. In principle,
	
	$\vec{\Theta}$ can be estimated from the integrated likelihood:
	
	\begin{equation*}
	
	\int_{\vec{X}^T} L(\vec{Y}^T \cond \vec{X}^T, \vec{Z}^T,
	
	\vec{\Theta}) L(\vec{X}^T \cond \vec{Z}^T, \vec{\Theta}^T) \ud \vec{X}^T
	
	
	\end{equation*}
	
	where $\vec{X}^T$ represents $\{\vec{X}_t; t = 0, \ldots,
	
	T\}$. Frequentist and Bayesian methods are discussed. For example,
	under
	
	the normal model the Kalman filter (linear) and extended Kalman filter
	
	(non-linear) can be used to estimate $\vec{\Theta}$ from $\vec{Y}^T$
	
	and $\vec{Z}^T$ alone, but otherwise this may be intractable. A
	
	Bayesian approach in which inference is based on the joint posterior
	
	$\Pr(\vec{\Theta},\vec{X}^T \cond \vec{Y}^T,\vec{Z}^T)$ is suggested.
	
	
	An example of the model formulation is given when catch-at-age data
	
	and some supplementary information is available. Technical challenges
	
	relating to estimation of many parameters is also discussed, and
	
	identifiability issues are mentioned.},
  timestamp = {2010.04.30}
}

