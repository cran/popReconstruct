@ARTICLE{BrooksCatchpoleMorgan_Bayesian_SS2000,
  author = {Brooks, S. P. and Catchpole, E. A. and Morgan, B. J. T.},
  title = {{Bayesian} Animal Survival Estimation},
  journal = {Statistical Science},
  year = {2000},
  volume = {15},
  pages = {357--376},
  number = {4},
  abstract = {We present the Bayesian approach to estimating parameters associated
	with animal survival on the basis of data arising from mark recovery
	and recapture studies. We provide two examples, beginning with a
	discussion of band-return models and examining data gathered from
	observations of blue winged teal (Anas discors), ringed as nestlings.
	We then look at open population recapture models, focusing on the
	Cormack-Jolly-Seber model, and examine this model in the context
	of a data set on European dippers (Cinclus cinclus). The Bayesian
	procedures are shown to be straightforward and provide a convenient
	framework for model-averaging, which incorporates the uncertainty
	due to model selection into the inference process. Sufficient detail
	is provided so that readers who wish to employ the Bayesian approach
	in this field can do so with ease. An example of BUGS code is also
	provided.},
  copyright = {Copyright © 2000 Institute of Mathematical Statistics},
  issn = {08834237},
  jstor_articletype = {primary_article},
  jstor_formatteddate = {Nov., 2000},
  publisher = {Institute of Mathematical Statistics},
  review = {*INTRODUCTION*
	
	- Provides a review and summary of methods used by Brooks, Catchpole,
	Morgan \& Co. in all their mark-recapture studies.
	
	- Also provides many references for Bayesian estimation in this model.
	
	
	*BAND-RETURN MODELS*
	
	- Likelihood model for band return models described:
	
	-- Parameters are survival in first year of life, survival after first
	year of life and reporting probability.
	
	-- Models labeled in x/y/z format to describe how each parameter depends
	on age "A", time "T" or is constant "C". E.g., T/A/T means 0-1 yr
	old survival and reporting prob dependent on time, adult survival
	prob dependent on age.
	
	-- Number of marked animals recovered when dead is a product of multinomial
	likelihoods.
	
	- Priors on all parameters are betas, since the parameters are all
	probabilities.
	
	- Sensitivity to choice of prior:
	
	-- By fitting the C/C/T model to some blue-winged teal data as an
	example, it is shown that choice of prior can have a significant
	effect on posterior parameter estimates for parameters for which
	there is little data, e.g., the survival probabilities at old ages.
	Importantly, changing the prior on reporting probabilities can influence
	posterior estimates of survival probabilities since they must be
	adjusted to match the data.
	
	-- Fitting the T/A/T model to the same data reveals sensitivity to
	the priors on the survival probabilties as well.
	
	- Posterior model probabilities are used to asses adequacy of various
	models. It is noted that these should be computed under a range of
	priors. There is an implicit penalty for many parameters built into
	the calculation of posterior model probabilities, while no such penalty
	is present in the calculation of Bayesian p-values. Hence, these
	two methods can lead to different model suggestions.
	
	
	*OPEN POPULATION CAPTURE-RECAPTURE MODELS*
	
	- In this design, adult animals are captured, marked and released.
	Captures are made at subsequent times and the number of already marked
	animals is recorded and the rest are marked for the first time. All
	are released and the procedure is repeated.
	
	- Probabilities of survival, probabilities of recapture and total
	population size can be estimated with appropriate models.
	
	- Beta priors are suggested, as for the band-return design.
	
	
	*DISCUSSION*
	
	- Need to be careful selecting priors: could use empirical priors
	based on data.},
  url = {http://www.jstor.org/stable/2676830}
}

@ARTICLE{BrooksCatchpoleMorganEtAl_Bayesian_B2000,
  author = {Brooks, S. P. and Catchpole, E. A. and Morgan, B. J. T. and Barry,
